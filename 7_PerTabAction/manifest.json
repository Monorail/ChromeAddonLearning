{
  "manifest_version": 2,
  "name": "Per Tab Action",
  "description": "This extension redirects all open pages to twitch.tv",
  "version": "1",
  "browser_action": {
  "default_icon": "icon.png"
},
"permissions": [
    "https://api.twitch.tv/*"
  ],
"background": {
  "scripts": ["background.js"]
}
}